<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>connectFilter</filter-name>
  	<filter-class>model.FilterConnect</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>connectFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>Home</servlet-name>
  	<servlet-class>controller.Home</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Pending</servlet-name>
  	<servlet-class>controller.Pending</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ChangeDetail</servlet-name>
  	<servlet-class>controller.ChangeDetail</servlet-class>
  </servlet>
    <servlet>
  	<servlet-name>RefeshPage</servlet-name>
  	<servlet-class>controller.RefeshPage</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Issue</servlet-name>
  	<servlet-class>controller.Issue</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Home</servlet-name>
  	<url-pattern>/Home</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>Pending</servlet-name>
  	<url-pattern>/Pending</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ChangeDetail</servlet-name>
  	<url-pattern>/ChangeDetail</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RefeshPage</servlet-name>
  	<url-pattern>/RefeshPage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Issue</servlet-name>
  	<url-pattern>/Issue</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  	<welcome-file>Home</welcome-file>
  </welcome-file-list>
</web-app>
